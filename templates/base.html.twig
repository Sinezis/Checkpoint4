		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body id="body-pd">
		<header class="header" id="header">
			{% include 'includes/_navbar.html.twig'%}
		</header>
		{# <main class="container"> #}
