{% extends 'base.html.twig' %}

{% block title %}Project
{% endblock %}

{% block body %}
	<h1>Project</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ project.id }}</td>
			</tr>
			<tr>
				<th>Image</th>
				<td>{{ project.image }}</td>
			</tr>
			<tr>
				<th>Date</th>
				<td>{{ project.date }}</td>
			</tr>
			<tr>
				<th>Lien</th>
				<td>{{ project.lien }}</td>
			</tr>
			<tr>
				<th>Nom</th>
				<td>{{ project.nom }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ project.description }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_project_index') }}">back to list</a>

	<a href="{{ path('app_project_edit', {'id': project.id}) }}">edit</a>

	{{ include('project/_delete_form.html.twig') }}
{% endblock %}
